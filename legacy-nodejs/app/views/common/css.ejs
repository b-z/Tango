<link rel="shortcut icon" href="favicon.ico">
<meta http-equiv="content-type" content="text/html; charset=utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
<link rel="stylesheet" href="css/materialize.min.css">
<link rel="stylesheet" href="css/common.css">
<link rel="stylesheet" href="css/color.css">
<style>
    .input-field input[type=text]:focus {
        border-bottom: 1px solid rgb(<%- color_value %>);
        box-shadow: 0 1px 0 0 rgb(<%- color_value %>);
    }

    .input-field textarea:focus {
        border-bottom: 1px solid rgb(<%- color_value %>)!important;
        box-shadow: 0 1px 0 0 rgb(<%- color_value %>)!important;
    }

    .waves-effect.waves-main .waves-ripple {
        background-color: rgba(<%- color_value %>, 0.65);
    }

    .input-field input[type=text]:focus + label {
        color: rgb(<%- color_value %>);
    }

    .input-field textarea:focus + label {
        color: rgb(<%- color_value %>)!important;
    }

    [type="checkbox"]:checked+label:before {
        border-right: 2px solid rgb(<%- color_value_second %>);
        border-bottom: 2px solid rgb(<%- color_value_second %>);
    }

    footer a {
        color: rgb(<%- color_value_second %>);
    }

    .table-of-contents a:hover {
        border-left: 1px solid rgb(<%- color_value_second %>)!important;
    }

    .table-of-contents a.active {
        border-left: 2px solid rgb(<%- color_value_second %>)!important;
    }


</style>
